{
  "name": "@propps/bigquery-ts",
  "version": "0.5.1",
  "description": "Typescript interface generators for BigQuery schemas",
  "author": "Kalana Vithana @kvithana",
  "bin": {
    "bigquery-ts": "./bin/run"
  },
  "homepage": "https://github.com/propps-au/bigquery-ts",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "propps-au/bigquery-ts",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^5.12.0",
    "@oclif/core": "^1.20.4",
    "@oclif/plugin-help": "^5.1.19",
    "@oclif/plugin-plugins": "^2.1.12",
    "camelcase": "^6.3.0",
    "common-tags": "^1.8.2",
    "node-dir": "^0.1.17",
    "ramda": "^0.28.0",
    "zod": "^3.19.1",
    "zod-to-ts": "^0.2.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.13",
    "@types/chai": "^4.3.4",
    "@types/common-tags": "^1.8.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.4",
    "@types/node-dir": "^0.0.34",
    "@types/ramda": "^0.28.20",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^11.1.0",
    "mocha": "^9.2.2",
    "np": "^7.6.2",
    "oclif": "^2.7.0",
    "prettier": "^2.8.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "oclif": {
    "bin": "bigquery-ts",
    "dirname": "bigquery-ts",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test_disabled": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/propps-au/bigquery-ts/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
